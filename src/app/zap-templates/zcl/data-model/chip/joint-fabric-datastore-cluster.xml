<configurator xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../../zcl.xsd">
  <domain name="CHIP"/>
  <enum name="DatastoreStateEnum" type="enum8">
    <cluster code="0x0752"/>
    <item name="Pending" value="0x00"/>
    <item name="Committed" value="0x01"/>
    <item name="DeletePending" value="0x02"/>
    <item name="CommitFailed" value="0x03"/>
  </enum>

  <struct name="DatastoreStatusEntryStruct">
    <cluster code="0x0752"/>
    <item fieldId="0" name="State" type="DatastoreStateEnum" default="0x00" min="0x00" max="0x03"/>
    <item fieldId="1" name="UpdateTimestamp" type="epoch_s"/>
    <item fieldId="2" name="FailureCode" type="status" default="0"/>
  </struct>

  <struct name="DatastoreNodeKeySetEntry" apiMaturity="provisional" isFabricScoped="true">
    <cluster code="0x0752"/>
    <item fieldId="0" name="NodeID" type="node_id"/>
    <item fieldId="1" name="GroupKeySetID" type="int16u"/>
    <item fieldId="2" name="StatusEntry" type="DatastoreStatusEntryStruct"/>
  </struct>

  <enum name="DatastoreAccessControlEntryPrivilegeEnum" type="enum8">
    <cluster code="0x0752"/>
    <item name="View" value="0x01"/>
    <item name="ProxyView" value="0x02"/>
    <item name="Operate" value="0x03"/>
    <item name="Manage" value="0x04"/>
    <item name="Administer" value="0x05"/>
  </enum>

  <struct name="DatastoreGroupInformationEntryStruct" isFabricScoped="true">
    <cluster code="0x0752"/>
    <item fieldId="0" name="GroupID" type="int64u"/>
    <item fieldId="1" name="FriendlyName" type="char_string" length="32"/>
    <item fieldId="2" name="GroupKeySetID" type="int16u" isNullable="true" min="1" max="65534"/>
    <item fieldId="3" name="GroupCAT" type="int16u" isNullable="true" min="1" max="65534"/>
    <item fieldId="4" name="GroupCATVersion" type="int16u" isNullable="true" min="1" max="65534"/>
    <item fieldId="5" name="GroupPermission" type="DatastoreAccessControlEntryPrivilegeEnum" min="0x00" max="0x05"/>
  </struct>

  <struct name="DatastoreBindingTargetStruct">
    <cluster code="0x0752"/>
    <item fieldId="1" name="Node" type="node_id" optional="true"/>
    <item fieldId="2" name="Group" type="group_id" optional="true" min="1"/>
    <item fieldId="3" name="Endpoint" type="endpoint_no" optional="true"/>
    <item fieldId="4" name="Cluster" type="cluster_id" optional="true"/>
  </struct>

  <struct name="DatastoreEndpointBindingEntryStruct" isFabricScoped="true">
    <cluster code="0x0752"/>
    <item fieldId="0" name="NodeID" type="node_id"/>
    <item fieldId="1" name="EndpointID" type="endpoint_no"/>
    <item fieldId="2" name="ListID" type="int16u"/>
    <item fieldId="3" name="Binding" type="DatastoreBindingTargetStruct"/>
    <item fieldId="4" name="StatusEntry" type="DatastoreStatusEntryStruct"/>
  </struct>

  <struct name="DatastoreEndpointGroupIDEntryStruct" isFabricScoped="true">
    <cluster code="0x0752"/>
    <item fieldId="0" name="NodeID" type="node_id"/>
    <item fieldId="1" name="EndpointID" type="endpoint_no"/>
    <item fieldId="2" name="GroupID" type="group_id"/>
    <item fieldId="3" name="StatusEntry" type="DatastoreStatusEntryStruct"/>
  </struct>

  <struct name="DatastoreEndpointEntryStruct">
    <cluster code="0x0752"/>
    <item fieldId="0" name="EndpointID" type="endpoint_no"/>
    <item fieldId="1" name="NodeID" type="node_id"/>
    <item fieldId="2" name="FriendlyName" type="char_string" length="32"/>
    <item fieldId="3" name="StatusEntry" type="DatastoreStatusEntryStruct"/>
  </struct>

  <enum name="DatastoreAccessControlEntryAuthModeEnum" type="enum8">
    <cluster code="0x0752"/>
    <item name="PASE" value="0x01"/>
    <item name="CASE" value="0x02"/>
    <item name="Group" value="0x03"/>
  </enum>

  <struct name="DatastoreAccessControlTargetStruct">
    <cluster code="0x0752"/>
    <item fieldId="0" name="Cluster" type="cluster_id" isNullable="true"/>
    <item fieldId="1" name="Endpoint" type="endpoint_no" isNullable="true"/>
    <item fieldId="2" name="DeviceType" type="devtype_id" isNullable="true"/>
  </struct>

  <struct name="DatastoreAccessControlEntryStruct">
    <cluster code="0x0752"/>
    <item fieldId="1" name="Privilege" type="DatastoreAccessControlEntryPrivilegeEnum" isFabricSensitive="true" min="0x00" max="0x05"/>
    <item fieldId="2" name="AuthMode" type="DatastoreAccessControlEntryAuthModeEnum" isFabricSensitive="true" min="0x00" max="0x03"/>
    <item fieldId="3" name="Subjects" type="int64u" isNullable="true" array="true" isFabricSensitive="true"/>
    <item fieldId="4" name="Targets" type="DatastoreAccessControlTargetStruct" isNullable="true" array="true" isFabricSensitive="true"/>
  </struct>

  <struct name="DatastoreACLEntryStruct" isFabricScoped="true">
    <cluster code="0x0752"/>
    <item fieldId="0" name="NodeID" type="node_id"/>
    <item fieldId="1" name="ListID" type="int16u"/>
    <item fieldId="2" name="ACLEntry" type="DatastoreAccessControlEntryStruct"/>
    <item fieldId="3" name="StatusEntry" type="DatastoreStatusEntryStruct"/>
  </struct>

  <struct name="DatastoreNodeInformationEntryStruct" isFabricScoped="true">
    <cluster code="0x0752"/>
    <item fieldId="1" name="NodeID" type="node_id"/>
    <item fieldId="2" name="FriendlyName" type="char_string" length="32"/>
    <item fieldId="3" name="CommissioningStatusEntry" type="DatastoreStatusEntryStruct"/>
  </struct>

  <struct name="DatastoreAdministratorInformationEntryStruct" isFabricScoped="true">
    <cluster code="0x0752"/>
    <item fieldId="1" name="NodeID" type="node_id"/>
    <item fieldId="2" name="FriendlyName" type="char_string" length="32"/>
    <item fieldId="3" name="VendorID" type="vendor_id"/>
    <item fieldId="4" name="ICAC" type="long_octet_string" length="400"/>
  </struct>

  <enum name="DatastoreGroupKeySecurityPolicyEnum" type="enum8">
    <cluster code="0x0752"/>
    <item name="TrustFirst" value="0x00"/>
    <item name="CacheAndSync" value="0x01"/>
  </enum>

  <enum name="DatastoreGroupKeyMulticastPolicyEnum" type="enum8">
    <cluster code="0x0752"/>
    <item name="PerGroupID" value="0x00"/>
    <item name="AllNodes" value="0x01"/>
  </enum>

  <struct name="DatastoreGroupKeySetStruct">
    <cluster code="0x0752"/>
    <item fieldId="0" name="GroupKeySetID" type="int16u"/>
    <item fieldId="1" name="GroupKeySecurityPolicy" type="DatastoreGroupKeySecurityPolicyEnum" isFabricSensitive="true" min="0x00" max="0x01"/>
    <item fieldId="2" name="EpochKey0" type="octet_string" length="16" isNullable="true" isFabricSensitive="true" minLength="16"/>
    <item fieldId="3" name="EpochStartTime0" type="epoch_us" isNullable="true" isFabricSensitive="true"/>
    <item fieldId="4" name="EpochKey1" type="octet_string" length="16" isNullable="true" isFabricSensitive="true" minLength="16"/>
    <item fieldId="5" name="EpochStartTime1" type="epoch_us" isNullable="true" isFabricSensitive="true"/>
    <item fieldId="6" name="EpochKey2" type="octet_string" length="16" isNullable="true" isFabricSensitive="true" minLength="16"/>
    <item fieldId="7" name="EpochStartTime2" type="epoch_us" isNullable="true" isFabricSensitive="true"/>
    <item fieldId="8" name="GroupKeyMulticastPolicy" type="DatastoreGroupKeyMulticastPolicyEnum" isFabricSensitive="true" min="0x00" max="0x01"/>
  </struct>

  <struct name="DatastoreStatusStruct" apiMaturity="provisional" isFabricScoped="true">
    <cluster code="0x0752"/>
    <item fieldId="0" name="State" type="DatastoreStateEnum" default="0x00" min="0x00" max="0x03"/>
    <item fieldId="1" name="UpdateTimestamp" type="epoch_s"/>
    <item fieldId="2" name="FailureCode" type="status" default="0"/>
  </struct>

  <cluster apiMaturity="provisional">
    <domain>General</domain>
    <name>Joint Fabric Datastore</name>
    <code>0x0752</code>
    <define>JOINT_FABRIC_DATASTORE_CLUSTER</define>
    <description>Joint Fabric Datastore Cluster.</description>
    <client init="false" tick="false">true</client>
    <server init="false" tick="false">true</server>
    <globalAttribute code="0xFFFD" side="either" value="1"/>
    <attribute side="server" code="0x0000" name="AnchorRootCA" define="ANCHOR_ROOT_CA" type="octet_string">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x0001" name="AnchorNodeID" define="ANCHOR_NODE_ID" type="node_id">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x0002" name="AnchorVendorID" define="ANCHOR_VENDOR_ID" type="vendor_id">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x0003" name="FriendlyName" define="FRIENDLY_NAME" type="char_string" length="32">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x0004" name="GroupKeySetList" define="GROUP_KEY_SET_LIST" type="array" entryType="DatastoreGroupKeySetStruct">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x0005" name="GroupList" define="GROUP_LIST" type="array" entryType="DatastoreGroupInformationEntryStruct">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x0006" name="NodeList" define="NODE_LIST" type="array" entryType="DatastoreNodeInformationEntryStruct">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x0007" name="AdminList" define="ADMIN_LIST" type="array" entryType="DatastoreAdministratorInformationEntryStruct">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x0008" name="Status" define="STATUS_ENTRY" type="DatastoreStatusStruct">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x0009" name="EndpointGroupIDList" define="ENDPOINT_GROUP_ID_LIST" type="array" entryType="DatastoreEndpointGroupIDEntryStruct">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x000A" name="EndpointBindingList" define="ENDPOINT_BINDING_LIST" type="array" entryType="DatastoreEndpointBindingEntryStruct">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x000B" name="NodeKeySetList" define="NODE_KEY_SET_LIST" type="array" entryType="DatastoreNodeKeySetEntry">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x000C" name="NodeACLList" define="NODE_ACL_LIST" type="array" entryType="DatastoreACLEntryStruct">
      <access op="read" privilege="administer"/>
    </attribute>
    <attribute side="server" code="0x000D" name="NodeEndpointList" define="NODE_ENDPOINT_LIST" type="array" entryType="DatastoreEndpointEntryStruct">
      <access op="read" privilege="administer"/>
    </attribute>
    <command source="client" code="0x00" name="AddKeySet" isFabricScoped="true">
      <description>This command SHALL be used to add a KeySet to the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="GroupKeySet" type="DatastoreGroupKeySetStruct"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x01" name="UpdateKeySet" isFabricScoped="true">
      <description>This command SHALL be used to update a KeySet in the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="GroupKeySet" type="DatastoreGroupKeySetStruct"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x02" name="RemoveKeySet" isFabricScoped="true">
      <description>This command SHALL be used to remove a KeySet from the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="GroupKeySetID" type="int16u"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x03" name="AddGroup" isFabricScoped="true">
      <description>This command SHALL be used to add a group to the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="GroupID" type="group_id"/>
      <arg id="1" name="FriendlyName" type="char_string" length="32"/>
      <arg id="2" name="GroupKeySetID" type="int16u" isNullable="true" min="1" max="65534"/>
      <arg id="3" name="GroupCAT" type="int16u" isNullable="true" min="1" max="65534"/>
      <arg id="4" name="GroupCATVersion" type="int16u" isNullable="true" min="1" max="65534"/>
      <arg id="5" name="GroupPermission" type="DatastoreAccessControlEntryPrivilegeEnum" min="0x00" max="0x05"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x04" name="UpdateGroup" isFabricScoped="true">
      <description>This command SHALL be used to update a group in the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="GroupID" type="group_id"/>
      <arg id="1" name="FriendlyName" type="char_string" length="32" isNullable="true"/>
      <arg id="2" name="GroupKeySetID" type="int16u" isNullable="true" min="1" max="65534"/>
      <arg id="3" name="GroupCAT" type="int16u" isNullable="true" min="1" max="65534"/>
      <arg id="4" name="GroupCATVersion" type="int16u" isNullable="true" min="1" max="65534"/>
      <arg id="5" name="GroupPermission" type="DatastoreAccessControlEntryPrivilegeEnum" min="0x00" max="0x05"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x05" name="RemoveGroup" isFabricScoped="true">
      <description>This command SHALL be used to remove a group from the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="GroupID" type="group_id"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x06" name="AddAdmin" isFabricScoped="true">
      <description>This command SHALL be used to add an admin to the Joint Fabric Datastore Cluster.</description>
      <arg id="1" name="NodeID" type="node_id"/>
      <arg id="2" name="FriendlyName" type="char_string" length="32"/>
      <arg id="3" name="VendorID" type="vendor_id"/>
      <arg id="4" name="ICAC" type="long_octet_string" length="400"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x07" name="UpdateAdmin" isFabricScoped="true">
      <description>This command SHALL be used to update an admin in the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="NodeID" type="node_id" isNullable="true"/>
      <arg id="1" name="FriendlyName" type="char_string" length="32" isNullable="true"/>
      <arg id="2" name="ICAC" type="long_octet_string" length="400" isNullable="true"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x08" name="RemoveAdmin" isFabricScoped="true">
      <description>This command SHALL be used to remove an admin from the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="NodeID" type="node_id"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x09" name="AddPendingNode" isFabricScoped="true">
      <description>The command SHALL be used to add a node to the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="NodeID" type="node_id"/>
      <arg id="1" name="FriendlyName" type="char_string" length="32"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x0A" name="RefreshNode" isFabricScoped="true">
      <description>The command SHALL be used to request that Datastore information relating to a Node is refreshed.</description>
      <arg id="0" name="NodeID" type="node_id"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x0B" name="UpdateNode" isFabricScoped="true">
      <description>The command SHALL be used to update the friendly name for a node in the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="NodeID" type="node_id"/>
      <arg id="1" name="FriendlyName" type="char_string" length="32"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x0C" name="RemoveNode" isFabricScoped="true">
      <description>This command SHALL be used to remove a node from the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="NodeID" type="node_id"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x0D" name="UpdateEndpointForNode" isFabricScoped="true">
      <description>This command SHALL be used to update the state of an endpoint for a node in the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="EndpointID" type="endpoint_no"/>
      <arg id="1" name="NodeID" type="node_id"/>
      <arg id="2" name="FriendlyName" type="char_string" length="32"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x0E" name="AddGroupIDToEndpointForNode" isFabricScoped="true">
      <description>This command SHALL be used to add a Group ID to an endpoint for a node in the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="NodeID" type="node_id"/>
      <arg id="1" name="EndpointID" type="endpoint_no"/>
      <arg id="2" name="GroupID" type="group_id"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x0F" name="RemoveGroupIDFromEndpointForNode" isFabricScoped="true">
      <description>This command SHALL be used to remove a Group ID from an endpoint for a node in the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="NodeID" type="node_id"/>
      <arg id="1" name="EndpointID" type="endpoint_no"/>
      <arg id="2" name="GroupID" type="group_id"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x10" name="AddBindingToEndpointForNode" isFabricScoped="true">
      <description>This command SHALL be used to add a binding to an endpoint for a node in the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="NodeID" type="node_id"/>
      <arg id="1" name="EndpointID" type="endpoint_no"/>
      <arg id="2" name="Binding" type="DatastoreBindingTargetStruct"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x11" name="RemoveBindingFromEndpointForNode" isFabricScoped="true">
      <description>This command SHALL be used to remove a binding from an endpoint for a node in the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="ListID" type="int16u"/>
      <arg id="1" name="EndpointID" type="endpoint_no"/>
      <arg id="2" name="NodeID" type="node_id"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x12" name="AddACLToNode" isFabricScoped="true">
      <description>This command SHALL be used to add an ACL to a node in the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="NodeID" type="node_id"/>
      <arg id="1" name="ACLEntry" type="DatastoreAccessControlEntryStruct"/>
      <access op="invoke" privilege="administer"/>
    </command>

    <command source="client" code="0x13" name="RemoveACLFromNode" isFabricScoped="true">
      <description>This command SHALL be used to remove an ACL from a node in the Joint Fabric Datastore Cluster.</description>
      <arg id="0" name="ListID" type="int16u"/>
      <arg id="1" name="NodeID" type="node_id"/>
      <access op="invoke" privilege="administer"/>
    </command>

  </cluster>
</configurator>
